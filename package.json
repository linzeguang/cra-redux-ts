{
  "name": "cra-redux-ts",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "build:analyzer": "craco build --analyzer",
    "test": "craco test",
    "preview": "npx serve -s build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier \"src/**/*.{js,jsx,ts,tsx,css,scss,less}\" --write",
    "prepare": "husky install",
    "version-tag:major": "npm version major -m \"UPGRADE to v%s\"",
    "version-tag:minor": "npm version minor -m \"UPGRADE to v%s\"",
    "version-tag:patch": "npm version patch -m \"UPGRADE to v%s\"",
    "version-tag:premajor": "npm version premajor -m \"UPGRADE to v%s\"",
    "version-tag:preminor": "npm version preminor -m \"UPGRADE to v%s\"",
    "version-tag:prepatch": "npm version prepatch -m \"UPGRADE to v%s\"",
    "version-tag:prerelease": "npm version prerelease -m \"UPGRADE to v%s\""
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@reduxjs/toolkit": "^1.9.2",
    "ahooks": "^3.7.5",
    "axios": "^1.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@craco/craco": "^7.0.0",
    "@craco/types": "^7.0.0",
    "@prerenderer/webpack-plugin": "^5.0.5",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.1",
    "prettier": "^2.8.4",
    "progress-bar-webpack-plugin": "^2.1.0",
    "puppeteer": "^19.7.3",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack-bundle-analyzer": "^4.7.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{css,scss,less}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
